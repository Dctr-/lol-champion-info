<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="main.IndividualChampionController">
    <children>
        <AnchorPane fx:id="mainWindow" prefHeight="700.0" prefWidth="500.0">
            <children>
                <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="&lt;-- Back"/>
                <Label fx:id="championName" layoutX="35.0" layoutY="46.0" prefHeight="30.0" prefWidth="447.0"
                       styleClass="champName" text="Champion Name + Blurb">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <ImageView fx:id="championSplash" fitHeight="280.0" fitWidth="154.0" layoutX="35.0" layoutY="82.0"
                           pickOnBounds="true" preserveRatio="true"/>
                <Label fx:id="lblAbilities" layoutX="220.0" layoutY="82.0" styleClass="abilitiesTitle" text="Abilities"
                       underline="true">
                    <font>
                        <Font size="21.0"/>
                    </font>
                </Label>
                <ImageView fx:id="attackDamageIcon" fitHeight="50.0" fitWidth="50.0" layoutX="213.0" layoutY="251.0"
                           pickOnBounds="true" preserveRatio="true"/>
                <ImageView fx:id="abilityPowerIcon" fitHeight="50.0" fitWidth="50.0" layoutX="308.0" layoutY="251.0"
                           pickOnBounds="true" preserveRatio="true"/>
                <ImageView fx:id="defenseIcon" fitHeight="50.0" fitWidth="50.0" layoutX="407.0" layoutY="251.0"
                           pickOnBounds="true" preserveRatio="true"/>
                <Label fx:id="attackDamageValue" alignment="CENTER" contentDisplay="CENTER" layoutX="232.0"
                       layoutY="311.0" text="ADValue">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="abilityPowerValue" alignment="CENTER" contentDisplay="CENTER" layoutX="325.0"
                       layoutY="311.0" text="APValue">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="defenseValue" alignment="CENTER" contentDisplay="CENTER" layoutX="426.0" layoutY="311.0"
                       text="DValue">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="lblSkins" layoutX="213.0" layoutY="362.0" styleClass="abilitiesTitle" text="Skins">
                    <font>
                        <Font size="22.0"/>
                    </font>
                </Label>
                <ScrollPane fx:id="skinsScrollPane" hbarPolicy="ALWAYS" layoutX="21.0" layoutY="403.0"
                            nodeOrientation="LEFT_TO_RIGHT" pannable="true" prefHeight="272.0" prefWidth="459.0"
                            vbarPolicy="NEVER">
                    <content>
                        <TilePane fx:id="skinsTilePane" alignment="CENTER" prefHeight="114.0" prefWidth="457.0"/>
                    </content>
                </ScrollPane>
                <RadioButton fx:id="favouriteButton" layoutX="382.0" layoutY="16.0" mnemonicParsing="false"
                             text="Favourite">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </RadioButton>
                <Pane layoutX="189.0" layoutY="124.0" prefHeight="31.0" prefWidth="304.0">
                    <children>
                        <ImageView fx:id="qImg" fitHeight="30.0" fitWidth="30.0" layoutX="30.0" layoutY="1.0"
                                   pickOnBounds="true" preserveRatio="true"/>
                        <Label fx:id="qAbilityLabel" layoutX="75.0" layoutY="4.0" prefHeight="20.0" prefWidth="241.0"
                               text="qLabel">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </Pane>
                <Pane layoutX="189.0" layoutY="150.0" prefHeight="31.0" prefWidth="304.0">
                    <children>
                        <ImageView fx:id="wImg" fitHeight="30.0" fitWidth="30.0" layoutX="30.0" layoutY="1.0"
                                   pickOnBounds="true" preserveRatio="true"/>
                        <Label fx:id="wAbilityLabel" layoutX="75.0" layoutY="6.0" prefHeight="20.0" prefWidth="241.0"
                               text="wLabel">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </Pane>
                <Pane layoutX="201.0" layoutY="181.0" prefHeight="31.0" prefWidth="304.0">
                    <children>
                        <ImageView fx:id="eImg" fitHeight="30.0" fitWidth="30.0" layoutX="18.0" layoutY="1.0"
                                   pickOnBounds="true" preserveRatio="true"/>
                        <Label fx:id="eAbilityLabel" layoutX="65.0" layoutY="4.0" prefHeight="20.0" prefWidth="241.0"
                               text="eLabel">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </Pane>
                <Pane layoutX="189.0" layoutY="212.0" prefHeight="31.0" prefWidth="304.0">
                    <children>
                        <ImageView fx:id="rImg" fitHeight="30.0" fitWidth="30.0" layoutX="30.0" layoutY="-3.0"
                                   pickOnBounds="true" preserveRatio="true"/>
                        <Label fx:id="rAbilityLabel" layoutX="75.0" layoutY="4.0" prefHeight="20.0" prefWidth="241.0"
                               text="rLabel">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
